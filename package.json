{
  "name": "angular-template",
  "version": "0.1.0",
  "author": "pureye4u",
  "description": "Web UI Starter - With angular 4.0.3",
  "license": "MIT",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pureye4u/angular-template.git"
  },
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -env loc -host 0.0.0.0 -aot",
    "play": "ng serve -env loc -host 0.0.0.0",
    "build": "npm run build:prd-aot",
    "build:loc": "ng build -env loc",
    "build:dev": "ng build -env dev",
    "build:stg": "ng build -env stg",
    "build:prd": "ng build -env prd",
    "build:loc-aot": "ng build -env loc -aot",
    "build:dev-aot": "ng build -env dev -aot",
    "build:stg-aot": "ng build -env stg -aot",
    "build:prd-aot": "ng build -env prd -aot",
    "test": "ng test",
    "lint": "ng lint",
    "lint:ci": "ng lint --format checkstyle > ./reports/checkstyle.xml",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "4.0.3",
    "@angular/common": "4.0.3",
    "@angular/compiler": "4.0.3",
    "@angular/core": "4.0.3",
    "@angular/forms": "4.0.3",
    "@angular/http": "4.0.3",
    "@angular/platform-browser": "4.0.3",
    "@angular/platform-browser-dynamic": "4.0.3",
    "@angular/router": "4.0.3",
    "core-js": "2.4.1",
    "rxjs": "5.3.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.3",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.13",
    "codelyzer": "2.0.1",
    "css-loader": "0.27.3",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "node-sass": "4.5.2",
    "postcss-loader": "1.3.3",
    "protractor": "5.1.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "ts-node": "2.0.0",
    "tslint": "4.5.1",
    "typescript": "2.2.2"
  }
}
